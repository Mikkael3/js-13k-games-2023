{
  "name": "js13k-games-2023",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "lint": "tsc --noEmit && eslint --ext .ts .",
    "zip": "zip -r -9 -j game.zip dist/*",
    "build-zip": "rm -rf dist;rm -rf game.zip; npm run build; npm run zip; du -b game.zip"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "eslint": "^8.45.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^5.0.0",
    "prettier": "^3.0.0",
    "typescript": "^5.0.2",
    "vite": "^4.4.0",
    "vite-tsconfig-paths": "^4.2.0"
  }
}